{
    "redirects": [
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "www.calcuri.com"
          }
        ],
        "destination": "https://calcuri.com/:1",
        "permanent": true
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install"
  }